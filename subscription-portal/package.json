{
	"name": "subscription-portal",
	"version": "1.0.0",
	"description": "Subscription Portal is a React based dashboard with usage metrics for car owners.",
	"devDependencies": {
		"liferay-npm-bundler": "^2.12.0",
		"liferay-npm-build-support": "^2.12.0",
		"copy-webpack-plugin": "4.6.0",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1",
		"babel-cli": "6.26.0",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"babel-loader": "7.1.5"
	},
	"dependencies": {
		"prettier": "^1.18.2",
		"prop-type": "0.0.1",
		"react": "16.8.6",
		"react-dom": "16.8.6"
	},
	"scripts": {
		"build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
		"copy-assets": "lnbs-copy-assets",
		"format": "prettier --write 'src/*.js'",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "category.custom",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": true,
		"javax.portlet.name": "subscription-portal",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.display-name": "Subscription Portal"
	},
	"prettier": {
		"bracketSpacing": false,
		"endOfLine": "lf",
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"main": "index.js"
}
