{
  "name": "dealer-template",
  "description": "Liferay Fragments project",
  "version": "1.0.0",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "liferay",
    "liferay-fragments"
  ],
  "scripts": {
    "add-collection": "yo liferay-fragments:collection",
    "add-fragment": "yo liferay-fragments:fragment",
    "build-banner": "node-sass src/scss/banner.scss src/dealer-collection/banner/styles.css",
    "build-browse-vehicles": "node-sass src/scss/browse-vehicles.scss src/dealer-collection/browse-vehicles/styles.css",
    "build-car-features": "node-sass src/scss/car-features.scss src/dealer-collection/car-features/styles.css",
    "build-customer-review": "node-sass src/scss/customer-review.scss src/dealer-collection/customer-review/styles.css",
    "build-footer": "node-sass src/scss/footer.scss src/dealer-collection/footer/styles.css",
    "build-global-styles": "node-sass src/scss/global-styles.scss src/dealer-collection/global-styles/styles.css",
    "build-header": "node-sass src/scss/header.scss src/dealer-collection/header/styles.css",
    "build-location-info": "node-sass src/scss/location-info.scss src/dealer-collection/location-info/styles.css",
    "build-quick-links": "node-sass src/scss/quick-links.scss src/dealer-collection/quick-links/styles.css",
    "build-search-filters": "node-sass src/scss/search-filters.scss src/dealer-collection/search-filters/styles.css",
    "build-css-all": "npm run build-banner && npm run build-browse-vehicles && npm run build-car-features && npm run build-customer-review && npm run build-footer && npm run build-global-styles && npm run build-header && npm run build-location-info && npm run build-quick-links && npm run build-search-filters",
    "compress": "yo liferay-fragments:compress",
    "export": "yo liferay-fragments:export",
    "import": "yo liferay-fragments:import",
    "import:watch": "yo liferay-fragments:import --watch",
    "preview": "yo liferay-fragments:preview"
  },
  "devDependencies": {
    "generator-liferay-fragments": "^1.2.1",
    "yo": "^3.1.0"
  },
  "dependencies": {
    "node-sass": "^4.12.0"
  }
}